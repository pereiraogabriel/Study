#include <stdio.h>

int main()
{
    int l1, l2, l3;
    
    printf("Informe os 3 Lados: ");
    scanf("%d", & l1);
    scanf("%d", & l2);
    scanf("%d", & l3);
    
    IF (l1 > l2 + l3 || l2 > l1 + l3 || l3 > l1 + l2)
        printf("Não é triangulo");    
            IF (l1 == l2 && l2 == l3)
            printf("Equilatero");
        ELSE
            IF (l1 == l2 || l2 == l3 || l1 == l3)
            printf("Isosceles");
        ELSE
        printf("Escaleno");
   
    return 0;
}
